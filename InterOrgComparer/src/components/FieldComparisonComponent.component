        <apex:form id="frmField">
         <apex:stylesheet value="{!URLFOR($Resource.InterOrgComparerResources, 'css/compare.css')}" />
         <apex:stylesheet value="{!URLFOR($Resource.InterOrgComparerResources, 'css/SnapItMetadataXplorerStyle.css')}" />
         <table>
			<tr>
			<td><apex:outputlabel value="{!$Label.Source_Object}" style="font-weight:bold;"
                                styleClass="fontSelectBold" /></td>
			<td> <apex:selectList id="sourceObject" styleClass="generalNormalLogin"
                                value="{!sourceObjectName}" size="1" >
               	<apex:actionSupport event="onchange"
							action="{!matchingTargetObjects}"
							status="loadingStatus1" rerender="targetObject" />                 
                  <apex:selectOptions value="{!sourceObjectList}"  /> 
           </apex:selectList></td>
			<td><apex:outputlabel value="{!$Label.Target_Object}"
                                styleClass="fontSelectBold"  style="margin-left: 25px;font-weight:bold;"/></td>
			<td><apex:selectList id="targetObject" styleClass="generalNormalLogin"
                                value="{!targetObjectName}" size="1" >
                  <apex:selectOptions value="{!targetObjectList}"  />
           </apex:selectList></td>
           <td><apex:actionStatus id="loadingStatus1"
							onstart="document.body.style.cursor='wait';return true;"
							onStop="document.body.style.cursor='default';return true;"
							startStyleClass="generalNormal">
							<apex:facet name="start">
								<center>
									<img src="/img/loading.gif" />&nbsp;
								</center>
							</apex:facet>
							<apex:facet name="stop" />
						</apex:actionStatus>
					</td>
			<td><apex:commandButton style="margin-left: 25px;" value="{!$Label.Show_Report}" action="{!fieldMetadataComparison}"  
                        status="loadingStatus" rerender="frmField" styleClass="fontSelectBold"/></td>
                        
                <td> <apex:actionStatus id="loadingStatus"
		onstart="document.body.style.cursor='wait';return true;"
		onStop="document.body.style.cursor='default';return true;"
		startStyleClass="generalNormal">
		<apex:facet name="start">
			<center>
				<img src="/img/loading.gif" />&nbsp;Processing...
			</center>
		</apex:facet>
		<apex:facet name="stop" />
	</apex:actionStatus></td>        
			</tr>
	 </table>
